<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaLoginController">
   <children>
      <Pane styleClass="root">
      		<children>
      				<Label layoutX="24.0" layoutY="8.0" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
      				<TextField fx:id="txtLogin" layoutX="65.0" layoutY="5.0" prefHeight="26.0" prefWidth="182.0" />
      				<Label layoutX="23.0" layoutY="38.0" text="Senha">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
      				<TextField fx:id="txtSenha" layoutX="65.0" layoutY="35.0" prefHeight="26.0" prefWidth="182.0" />
      				<Button fx:id="btnLogin" layoutX="5.0" layoutY="65.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="118.0" text="Entrar">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
      				<Button fx:id="btnCadastro" layoutX="128.0" layoutY="65.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="118.0" text="Cadastro">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
      		</children>
         <padding>
            <Insets bottom="5.0" right="5.0" />
         </padding>
      </Pane>
   </children>
</AnchorPane>
